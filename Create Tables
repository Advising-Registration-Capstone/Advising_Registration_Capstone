CREATE TALBE COURSE (
COUSRE_ID INT PRIMARY KEY,
COURSE_CREDITS INT,
COURSE_TITLE VARCHAR(36),
COURSE_DESC VARCHAR(128),
COURSE_PREREQ INT 
);

CREATE TALBE PREREQUISITES (
PREREQ_ID INT PRIMARY KEY,
COURSE_ID INT,
REQ_ID INT 
);

CREATE TABLE SECTION (
SECTION_ID INT PRIMARY KEY,
INSTRUCTOR_ID INT,
COURSE_ID INT,
SECTION_SEMESTER VARCHAR(20),
SECTION_YEAR VARCHAR(4),
SECTION_BUILDING VARCHAR(25),
SECTION_ROOM VARCHAR(4),
SECTION_DAYS VARCHAR(9),
SECTION_TIME VARCHAR(18)
);

CREATE TABLE PROFESSOR(
PROF_ID INT PRIMARY KEY,
PROF_LNAME VARCHAR(35),
PROF_FNAME VARCHAR(35),
PROF_OFFICE_LOC VARCAHR(30),
PROF_OFFICE_PHN VARCHAR(15)
);

CREATE TABLE STUDENT (
STU_ID INT PRIMARY KEY,
STU_FNAME VARCHAR(30),
STU_LNAME VARCHAR(35),
STU_EMAIL VARCHAR(64),
STU_PHONE VARCHAR(15),
STU_ADDRESS VARCHAR(60),
STU_CITY VARCHAR(128),
STU_STATE (25),
--STU_COUNTRY?
STU_COUNTY VARCHAR(40),
ADV_ID INT
);

CREATE TABLE ENROLLMENT (
ENROLLMENT_ID INT PRIMARY KEY,
SECTION_ID INT, 
COURSE_ID INT,
STUDENT_ID INT,
FINAL_GRADE CHAR,
ENROLLMENT_STATUS VARCHAR(15)
);
